    "def f_func(dt):\n",
    "    if dt!=\"completed\":  \n",
    "        return False\n",
    "\n",
    "f=data[\"Status\"].apply(f_func)\n",
    "\n",
    "\n",
    "def t_func1(dt):\n",
    "    if dt[\"Age(clean)\"] == True or dt[\"Height(clean)\"]==True or dt[\"Initial Weight\"]==True or dt[\"Lowest Weight\"]==True or dt[\"Target Weight\"]==True:\n",
    "        return True\n",
    "\n",
    "t_1=data.isnull().apply(t_func1,axis=\"columns\")\n",
    "\n",
    "\n",
    "def t_func2(dt):\n",
    "    if dt[\"Height(clean)\"] < 140 or dt[\"Height(clean)\"] > 200:\n",
    "        return True\n",
    "\n",
    "t_2=data.apply(t_func2,axis=\"columns\")\n",
    "\n",
    "\n",
    "def t_func3(dt):\n",
    "    if dt[\"BMI\"]<18.5 or dt[\"BMI\"]>30:\n",
    "        return True\n",
    "\n",
    "t_3=data.apply(t_func3,axis=\"columns\")\n",
    "\n",
    "\n",
    "def t_func4(dt):\n",
    "    if dt[\"Weight Loss(goal)\"]<0:\n",
    "        return True\n",
    "\n",
    "t_4=data.apply(t_func4,axis=\"columns\")\n",
    "\n",
    "ft_df=pd.DataFrame({\"f\":f,\n",
    "             \"t_1\":t_1,\n",
    "             \"t_2\":t_2,\n",
    "             \"t_3\":t_3,\n",
    "             \"t_4\":t_4})\n",
    "\n",
    "def ft_func(dt):\n",
    "    if dt[\"f\"]==False:\n",
    "        return False\n",
    "    else:\n",
    "        if dt[\"t_1\"] == True or dt[\"t_2\"] == True or dt[\"t_3\"] == True or dt[\"t_4\"] == True:\n",
    "            return True\n",
    "        else :\n",
    "            return False\n",
    "        \n",
    "data[\"invalid\"]=ft_df.apply(ft_func, axis=\"columns\")\n",
