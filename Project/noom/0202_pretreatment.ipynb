    "def com_func(dt):\n",
    "    if dt[\"Status\"]==\"completed\" and dt[\"Weight Loss(current)\"]>=10 and dt[\"BMI\"]>=30 and dt[\"Weight Loss(current)\"]>dt[\"Weight Loss(goal)\"]:\n",
    "        return True\n",
    "    \n",
    "com=data.apply(com_func, axis = \"columns\")\n",
    "    \n",
    "def wei_null_func(dt):\n",
    "    if dt[\"Weight Loss(goal)\"]==False and dt[\"Weight Loss(current)\"]==False and dt[\"BMI\"]==False:\n",
    "        return True\n",
    "\n",
    "wei_null=data.isnull().apply(wei_null_func, axis=\"columns\")\n",
    "\n",
    "def vip_func(dt):\n",
    "    if dt[\"com\"]==True and dt[\"wei_null\"]==True:\n",
    "        return True\n",
    "\n",
    "vip_dt=pd.DataFrame({\"com\":com, \"wei_null\":wei_null})\n",
    "data[\"VIP\"]=vip_dt.apply(vip_func, axis=\"columns\")\n",
    "data[data[\"VIP\"]==True]\n",
    "\n",
    "\n"
   ]
