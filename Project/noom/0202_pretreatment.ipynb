    "def age_g_func(dt):\n",
    "    if dt>=55:\n",
    "        return \"55~99\"\n",
    "    elif dt>=45:\n",
    "        return \"45~54\"\n",
    "    elif dt>=36:\n",
    "        return \"36~44\"\n",
    "    elif dt>=25:\n",
    "        return \"25~35\"\n",
    "    elif dt>=18:\n",
    "        return \"18~24\"\n",
    "    else:\n",
    "        return \"00~17\"\n",
    "        \n",
    "data[\"Age(Group)\"]=data[\"Age(clean)\"].apply(age_g_func)\n",
    "\n",
    "data[\"Gender(clean)\"]=data1[\"Gender(clean)\"]\n",
    "\n",
    "\n",
    "pvt=pd.pivot_table(data,\n",
    "               index=['Gender(clean)','Age(Group)'],\n",
    "               values='Age(clean)',\n",
    "               columns='Status',\n",
    "               fill_value=0,\n",
    "               aggfunc='count')\n",
    "\n",
    "def pvt_plus_col(pv):\n",
    "    pvt_sum=pv.sum()\n",
    "    pvt_com_prob=pv[1]/pvt_sum\n",
    "    return [pvt_sum, pvt_com_prob]\n",
    "\n",
    "pvt_df=pvt.apply(pvt_plus_col,axis=1)\n",
    "\n",
    "pvt[\"total\"]=pvt_df.str[0]\n",
    "pvt[\"conversion\"]=pvt_df.str[1]\n",
    "pvt"
   ]
