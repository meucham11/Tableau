    "def coach_tcvcc(dt):\n",
    "    dt_sum=dt.sum()\n",
    "    dt_conversion=dt[1]/dt_sum\n",
    "    dt_cancel=(dt[0]+dt[2])/dt_sum\n",
    "    return [dt_sum,dt_conversion,dt_cancel]\n",
    "\n",
    "coach_result=pvt_coach.apply(coach_tcvcc,axis=1)\n",
    "pvt_coach[\"total\"]=coach_result.str[0]\n",
    "pvt_coach[\"conversion rate\"]=coach_result.str[1]\n",
    "pvt_coach[\"cancellation rate\"]=coach_result.str[2]\n",
    "\n",
    "pvt_result=pvt_coach[pvt_coach[\"total\"]>=100]\n",
    "\n",
    "\n",
    "coach_conv_top=pvt_result.sort_values(by=\"conversion rate\",ascending=False)\n",
    "coach_conv_top.head()"
   ]
